                    <Button x:Name="btn_rect" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkRectanger" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/rect.fw.png" Stretch="Fill"/>
