                <Canvas x:Name="cnv_menu_right_panel">
                    <Image x:Name="panel_menu_right_outer" Source="/Assets/Icons/panel_outer.fw.png" Canvas.Left="80" Canvas.Top="80" IsHitTestVisible="False">
                        <Image.RenderTransform>
                            <CompositeTransform Rotation="90" CenterX="320" CenterY="320"/>
                        </Image.RenderTransform>
                    </Image>
                    <Image x:Name="panel_menu_right_middle" Source="/Assets/Icons/panel_middle.fw.png" Canvas.Left="150" Canvas.Top="150" IsHitTestVisible="False">
                        <Image.RenderTransform>
                            <CompositeTransform Rotation="90" CenterX="250" CenterY="250"/>
                        </Image.RenderTransform>
                    </Image>
                    <Image x:Name="panel_menu_right_inner" Source="/Assets/Icons/panel_inner.fw.png" Canvas.Left="220" Canvas.Top="220" IsHitTestVisible="False">
                        <Image.RenderTransform>
                            <CompositeTransform Rotation="90" CenterX="180" CenterY="180"/>
                        </Image.RenderTransform>
                    </Image>
                    <Button x:Name="btn_menu" Width="70" Height="70" Padding="0" Canvas.Left="320" Canvas.Top="320" Click="OnClkMenu" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="60" Height="60" Source="/Assets/Icons/feature.settings.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_clear" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="appMenu_clear_Click" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/refresh.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_modify" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkMod" IsEnabled="False" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/modify.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_save" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnCLkSave" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/save.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_front" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColorChange" Template="{StaticResource tmp_MenuButton}" Background="{Binding brush_foreground}" Content="S"/>
                    <Button x:Name="btn_board" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColorChange" Template="{StaticResource tmp_MenuButton}" Background="{Binding brush_background}" Content="B"/>
                    <Button x:Name="btn_fill" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColorChange" Template="{StaticResource tmp_MenuButton}" Background="{Binding brush_fill}" Content="F"/>
                    <Button x:Name="btn_closure" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkClosure" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/closure.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_smart" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="appBtn_smart_Click" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/smart.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_free" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="appBtn_free_Click" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/free.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_line" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="appBtn_line_Click" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/line.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_rect" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkRectanger" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/rect.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_ecllipse" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="appBtn_circle_Click" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/ellipse.fw.png" Stretch="Fill"/>
                    </Button>
                    <Button x:Name="btn_polygon" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkPolygon" Template="{StaticResource tmp_MenuButton}">
                        <Image Width="40" Height="40" Source="/Assets/Icons/polygon.fw.png" Stretch="Fill"/>
                    </Button>

                    <Button x:Name="btn_color_black" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Black" Content="Bk"/>
                    <Button x:Name="btn_color_white" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="White" Content="Wh"/>
                    <Button x:Name="btn_color_red" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Red" Content="Rd"/>
                    <Button x:Name="btn_color_blue" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="#FF1E5AF0" Content="Bl"/>
                    <Button x:Name="btn_color_yellow" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Yellow" Content="Ye"/>
                    <Button x:Name="btn_color_green" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Lime" Content="Gr"/>
                    <Button x:Name="btn_color_tran" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Content="Tr">
                        <Button.Foreground>
                            <ImageBrush ImageSource="/Assets/Icons/transparent.fw.png"/>
                        </Button.Foreground>
                    </Button>
                </Canvas>
            </Grid>
        </Grid>
        <!--取消注释，以显示对齐网格，从而帮助确保
            控件在公用边界上对齐。图像在系统栏中显示时的
            上边距为 -32px。如果隐藏了系统栏，则将此值设为 0
            (或完全删除边距)。

            在发送之前删除此 XAML 和图像本身。-->
