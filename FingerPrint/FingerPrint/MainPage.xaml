                    </Button>

                    <Button x:Name="btn_mod"  Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Template="{StaticResource tmp_MenuButton}" MouseMove="OnModMove" MouseEnter="OnModHold" MouseLeave="OnModRelease">
                        <Image Width="40" Height="40" Source="/Assets/Icons/modify.fw.png" Stretch="Fill"/>
                    </Button>

                    <Button x:Name="btn_color_black" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Black" Content="Bk"/>
                    <Button x:Name="btn_color_white" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="White" Content="Wh"/>
                    <Button x:Name="btn_color_red" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Red" Content="Rd"/>
                    <Button x:Name="btn_color_blue" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="#FF1E5AF0" Content="Bl"/>
                    <Button x:Name="btn_color_yellow" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Yellow" Content="Ye"/>
                    <Button x:Name="btn_color_green" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Foreground="Lime" Content="Gr"/>
                    <Button x:Name="btn_color_tran" Width="60" Height="60" Padding="0" Canvas.Left="400" Canvas.Top="400" Click="OnClkColor" Template="{StaticResource tmp_ColorButton}" Content="Tr">
                        <Button.Foreground>
                            <ImageBrush ImageSource="/Assets/Icons/transparent.fw.png"/>
                        </Button.Foreground>
                    </Button>
                    
                    <Slider Width="300" Height="300" Template="{StaticResource tmp_CanvasZoom}"></Slider>
                </Canvas>
            </Grid>
        </Grid>
    </Grid>

